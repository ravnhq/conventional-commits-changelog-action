{
  "name": "conventional-commits-release-info",
  "version": "1.0.0",
  "private": true,
  "description": "Github action to generate release info based on conventional commits",
  "//main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build src/index.ts --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "conventional-changelog": "^3.1.25",
    "conventional-recommended-bump": "^6.1.0"
  },
  "devDependencies": {
    "@ravnhq/prettier-config": "^1.0.0",
    "@types/conventional-changelog": "^3.1.1",
    "@types/git-semver-tags": "^4.1.1",
    "@types/node": "^16.10.5",
    "@typescript-eslint/parser": "^5.8.1",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^25.3.2",
    "jest": "^27.2.5",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.4.4"
  }
}
